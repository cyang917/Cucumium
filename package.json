{
  "name": "cucumium",
  "version": "1.0.0",
  "description": "Nodejs Test Automation  Framework(Nodejs +  Cucumber + Selenium + Appium + BrowserStack.",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyang917/Cucumium"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "cucumber": "^4.0.0",
    "cucumber-html-reporter": "^3.0.3",
    "chromedriver": "^2.35.0",
    "geckodriver": "^1.10.0",
    "selenium-webdriver": "^3.6.0",
    "webdriver-js-extender": "github:jayrepo/webdriver-js-extender#208ca66"
  },
  "scripts": {
    "report": "node ./reports/index.js",
    "test": "./node_modules/.bin/cucumber-js --format json:./reports/cucumber_report.json",
    "tag": "./node_modules/.bin/cucumber-js --format json:./reports/cucumber_report.json --tags"
  }
}
